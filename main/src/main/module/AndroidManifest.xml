<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.kcr.main"> <!-- To auto-complete the email text field in the login form with the user's emails -->
    <uses-permission android:name="android.permission.GET_ACCOUNTS" />
    <uses-permission android:name="android.permission.READ_PROFILE" />
    <uses-permission android:name="android.permission.READ_CONTACTS" />

    <application
        android:name="test.MyApplication"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/QMUI.Compat.NoActionBar">


        <activity android:name=".ui.activity.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".ui.activity.DetailsWelfareActivirt"
            android:launchMode="singleTask"
            android:theme="@style/AppTheme.Skid_2" />
        <activity android:name=".ui.activity.CapitalDetailedFormActivity" />
        <activity android:name=".ui.activity.CoinDetailedActivity" />
        <activity android:name=".ui.activity.GroundActivity" />
        <activity android:name=".ui.activity.MedalActivity" />
        <activity android:name=".ui.activity.setting.SettingActivity" />
        <activity android:name=".ui.activity.setting.FeedbackActivity" />
        <activity android:name=".ui.activity.setting.AboutActivity" />
    </application>
</manifest>